'''
radni_sati = float(input("Unesite broj radnih sati: "))
satnica = float(input("Unesite satnicu: "))
zarada = radni_sati * satnica
print("Korisnik je zaradio: " + str(zarada) + "eura")
'''

def total_euro(radni_sati, satnica):
    print("Ukupna zarada je: " + str(radni_sati * satnica) + " eura")

def main():
    radni_sati = float(input("Unesite broj radnih sati: "))
    satnica = float(input("Unesite satnicu: "))

    total_euro(radni_sati, satnica)


main()
